<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Hotels</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>OrderServlet</display-name>
    <servlet-name>OrderServlet</servlet-name>
    <servlet-class>com.linpeirou.www.view.OrderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OrderServlet</servlet-name>
    <url-pattern>/OrderServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CancelOrderServlet</display-name>
    <servlet-name>CancelOrderServlet</servlet-name>
    <servlet-class>com.linpeirou.www.view.CancelOrderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CancelOrderServlet</servlet-name>
    <url-pattern>/CancelOrderServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ReservaionServlet</display-name>
    <servlet-name>ReservaionServlet</servlet-name>
    <servlet-class>com.linpeirou.www.view.ReservaionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReservaionServlet</servlet-name>
    <url-pattern>/ReservaionServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>EnterFilter</filter-name>
    <filter-class>com.linpeirou.www.filter.EnterFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>EnterFilter</filter-name>
    <url-pattern>/ForCoustomer/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>AdministratorFilter</filter-name>
    <filter-class>com.linpeirou.www.filter.AdministratorFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AdministratorFilter</filter-name>
    <url-pattern>/ForAdministrator/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>SuperFilter</filter-name>
    <filter-class>com.linpeirou.www.filter.SuperFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>SuperFilter</filter-name>
    <url-pattern>/ForSuper/*</url-pattern>
  </filter-mapping>
  
</web-app>